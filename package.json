{
  "name": "dzb-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode mock",
    "build": "vite build",
    "build:mock": "vite build --mode mock",
    "deploy:gh-pages": "bash gh-pages-deploy.sh",
    "serve": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "prettier": "prettier --ignore-unknown --write .",
    "stylelint": "stylelint **/*.{vue,css,scss}",
    "stylelint:fix": "stylelint **/*.{vue,css,scss} --fix",
    "eslint": "eslint **/*.{vue,ts,js}",
    "eslint:fix": "eslint **/*.{vue,ts,js} --fix",
    "lint": "npm run prettier && npm run stylelint && npm run eslint && npm run type-check",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*": "npm run prettier",
    "*.{vue,ts,js}": [
      "npm run eslint:fix",
      "bash -c 'npm run type-check'"
    ],
    "*.{vue,css,scss}": "npm run stylelint:fix"
  },
  "dependencies": {
    "@vueform/multiselect": "^2.6.1",
    "@vueuse/core": "^7.6.2",
    "@vueuse/integrations": "^7.7.0",
    "2gis-maps": "^3.7.3",
    "focus-trap": "^6.7.3",
    "js-cookie": "^3.0.1",
    "ky": "^0.28.7",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "pinia": "^2.0.11",
    "vue": "^3.2.31",
    "vue-query": "^1.26.0",
    "vue-router": "^4.0.12",
    "vue-toastification": "^2.0.0-rc.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/vue3": "^7.0.18",
    "@storybook/vue3-vite": "^7.0.18",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.3.2",
    "@types/node": "^16.11.11",
    "@types/svgo": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.31.1",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.3.2",
    "dotenv": "^16.0.3",
    "esbuild": "^0.14.39",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^7.10.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.1",
    "postcss-html": "^1.4.1",
    "prettier": "^2.3.0",
    "sass": "^1.53.0",
    "storybook": "^7.0.18",
    "stylelint": "^14.15.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.3.2",
    "vite": "^4.3.8",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.5"
  }
}
