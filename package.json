{
  "name": "dzb-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode mock",
    "build": "npm run lint && vite build",
    "deploy:gh-pages": "bash gh-pages-deploy.sh",
    "serve": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "prettier": "prettier --ignore-unknown --write",
    "stylelint": "stylelint **/*.{vue,css}",
    "stylelint:fix": "stylelint **/*.{vue,css} --fix",
    "eslint": "eslint **/*.{vue,ts}",
    "eslint:fix": "eslint **/*.{vue,ts} --fix",
    "lint": "npm run prettier && npm run stylelint && npm run eslint && npm run type-check",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "npm run prettier",
    "*.{vue,ts}": "npm run eslint:fix",
    "*.{vue,css}": "npm run stylelint:fix"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@dafcoe/vue-collapsible-panel": "^0.2.0",
    "@vueform/multiselect": "^2.3.3",
    "@vueuse/core": "^7.6.2",
    "@vueuse/integrations": "^7.7.0",
    "focus-trap": "^6.7.3",
    "ky": "^0.28.7",
    "lodash": "^4.17.21",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^1.5.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.11",
    "@types/svgo": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.3.0",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.4",
    "vite-svg-loader": "^3.1.2",
    "vue-tsc": "^0.32.1"
  }
}
